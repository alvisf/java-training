	}
	
	class PaintBrush{
		Paint paint;
		public doPaint() {
			System.out.println(paint);
		}
		
	}
	
	abstract class Paint{
		
	}
	
	class RedPaint extends Paint{
		
	}
	
	class BluePaint extends Paint{
		
	}

}
