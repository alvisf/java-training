package day4;
import java.util.Scanner;
public class Ex11 {
	public static void FloydTriangle( )

	{

	int c, d, num = 1 ;

	for( c = 1 ; c <= 5 ; c++ )

	{

	for( d = 1 ; d <= c ; d++ )

	{

	System.out.print(num + " " );

	num++ ;

	}

	System.out.println( ) ;

	}

	}


}
